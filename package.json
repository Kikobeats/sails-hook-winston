    "isparta": "4.0.0",
    "mocha": "2.3.4",
    "sails": "0.11.3",
    "sinon": "1.17.2"
  },
  "engines": {
    "node": ">= 4.0.0",
    "npm": ">= 3.0.0"
  },
  "sails": {
    "isHook": true
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "publishConfig": {
    "tag": "latest"
  },
  "release": {
    "branch": "master"
  }
